<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link type="text/css" rel="stylesheet" href="stylesheets/materialize.min.css"  media="screen,projection"/>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="javascripts/workouts.js"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <title></title>
  </head>
  <body>
    <nav class="grey lighten-1">
      <div>
        <a href="/dashboard" class="brand-logo center">Ascetic</a>
        <ul id="nav-mobile" class="left hide-on-med-and-down">
          <li><a href="/exercises">Exercises</a></li>
          <li><a href="/workouts">Workouts</a></li>
        </ul>
      </div>
    </nav>

    <div class="container">
      <div class="col s12 m12 center-align">
        <h1>Workouts</h1>
      </div>
      <ul class="collection with-header">
        <% workouts.forEach((workout) => { %>
          <li class="collection-header" id="workoutHeader"><h6><%= workout.name %></h6></li>
          <% workout.exercises.forEach((exercise) => { %>
            <li class="collection-item exerciseShow">
              <strong><%= exercise.name %></strong>
            </li>
          <% }) %>
        <% }) %>
      </ul>
    </div>

    <script type="text/javascript" src="javascripts/materialize.min.js"></script>
  </body>
</html>
